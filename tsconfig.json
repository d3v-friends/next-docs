{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@app/*": [
        "./comp/app/*"
      ],
      "@css": [
        "./comp/global.scss"
      ],
      "@fn/*": [
        "./fn/*"
      ],
      "@cutil": [
        "./comp/cutil.ts"
      ],
      "@event/*": [
        "./comp/event/*"
      ],
      "@block/*": [
        "./comp/block/*"
      ],
      "@tag/*": [
        "./comp/tag/*"
      ],
      "@style": [
        "./comp/scss/index.scss"
      ],
      "@style/*": [
        "./comp/scss/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
